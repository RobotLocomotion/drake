<?xml version="1.0"?>

<robot name="MassSpringDamper">
  <material name="green">
    <color rgba=".3 .6 .4 1" />
  </material>
  <material name="red">
    <color rgba=".9 .1 0 1" />
  </material>
  <material name="blue">
    <color rgba="0 0 1 1" />
  </material>
  <link name="world"/>
  <link name="mass">
    <inertial> 
      <origin xyz="0 0 0" rpy="0 0 0" />
      <mass value="1" />
      <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0"/> 
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <box size="1 1 1" />
      </geometry>
      <material name="red" />
    </visual> 
  </link>
  <joint name="x" type="prismatic">
    <parent link="world"/>
    <child link="mass" />
    <axis xyz="1 0 0" />
  </joint>
  <force_element name="spring" type="LinearSpringDamper">
    <link1 link="world" xyz="0 0 0" />
    <link2 link="mass" xyz="0 0 0" />
    <stiffness value="10" />
    <damping value="1" />
  </force_element>
</robot>
