            </visual>
        </link>

		<!-- end of link list -->
		<!-- joint list -->
		
		<!-- WARNING: LIMITS NOT CORRECT -->
		<joint name="${prefix}joint_0" type="revolute">
            <parent link="${prefix}link_0"/>
            <child link="${prefix}link_1"/>
            <axis xyz="0 0 1"/>
            <origin rpy="0 0 0" xyz="0.020 0 0"/>
            <limit effort="100" lower="0" upper="1.055" velocity="100"/>
        </joint>
  
        <joint name="${prefix}joint_1" type="revolute">
            <parent link="${prefix}link_1"/>
            <child link="${prefix}link_2"/>
            <axis xyz="0 0 1"/>
            <origin rpy="0 0 -0.52" xyz="0.050 -.028 0"/>
            <limit effort="100" lower="0" upper="1.5708" velocity="100"/>
        </joint>
 
        <joint name="${prefix}joint_2" type="revolute">
            <parent link="${prefix}link_2"/>
            <child link="${prefix}link_3"/>
            <axis xyz="0 0 1"/>
            <origin rpy="0 0 0" xyz="0.039 0 0"/>
            <limit effort="100" lower="-0.7854" upper="3.1416" velocity="100"/>
        </joint>

        <joint name="${prefix}fourbar_0_joint_0" type="revolute">
            <axis xyz="0 0 1"/>
            <parent link="${prefix}link_0"/>
            <child link="${prefix}fourbar_0_link_1"/>
            <origin rpy="0 0 0" xyz="0.020 0 0"/>
            <limit effort="100" lower="-2.10" upper="-0.255" velocity="100"/>     
        </joint>

        <joint name="${prefix}fourbar_0_joint_1" type="continuous">
            <axis xyz="0 0 1"/>
            <parent link="${prefix}fourbar_0_link_1"/>
            <child link="${prefix}fourbar_0_link_2"/>
            <origin rpy="0 0 0" xyz=".02858 0 0"/>
            <limit effort="100" lower="1.092" upper="1.940" velocity="100"/>
        </joint>

        <joint name="${prefix}fourbar_0_joint_2" type="continuous">
            <axis xyz="0 0 1"/>
            <parent link="${prefix}link_1"/>
            <child link="${prefix}fourbar_0_link_3"/>
            <origin rpy="0 0 -0.52" xyz="0.050 -.028 0"/>
            <limit effort="100" lower="-2.111" upper="-0.199" velocity="100"/>     
        </joint>

        <loop_joint name="${prefix}fourbar_0_joint_3" type="continuous">
            <axis xyz="0 0 1"/>
            <link1 link="${prefix}fourbar_0_link_2" rpy="0 0 0" xyz=".05398 0 0"/>
            <link2 link="${prefix}fourbar_0_link_3" rpy="0 0 0" xyz=".01473 0 0"/>
        </loop_joint>
  
        <joint name="${prefix}fourbar_3_joint_0" type="continuous">
            <axis xyz="0 0 1"/>
            <parent link="${prefix}link_2"/>
            <child link="${prefix}fourbar_3_link_1"/>
