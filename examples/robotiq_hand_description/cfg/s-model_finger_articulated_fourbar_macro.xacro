        </loop_joint>

        <joint name="${prefix}fourbar_linkage_joint_0" type="continuous">
            <axis xyz="0 0 1"/>
            <parent link="${prefix}fourbar_0_link_3"/>
            <child link="${prefix}fourbar_linkage"/>
            <origin rpy="0 0 0" xyz=".01473 0 0"/>
            <limit effort="100" lower="1.4" upper="1.5" velocity="100"/>
        </joint>

        <loop_joint name="${prefix}fourbar_linkage_joint_1" type="continuous">
            <axis xyz="0 0 1"/>
            <link1 link="${prefix}fourbar_linkage" rpy="0 0 0" xyz=".015236 0 0"/>
            <link2 link="${prefix}fourbar_3_link_1" rpy="0 0 0" xyz=".02222 0 0"/>
        </loop_joint>
		<!-- end of joint list -->
        <!-- gazebo property list -->
        <gazebo reference="${prefix}link_0">
