				<material name="black"/>
			</visual>
			<collision>
	            <origin xyz="0.02 0.0025 -0.0005" rpy="0 0 0"/>
	            <geometry>
	                <box size=".018 .01 .03"/>
	            </geometry>
	            <material name="yellow"/>
	        </collision>
            <inertial>
                <origin xyz="0 0 0" rpy="0 0 0"/>
                <mass value="0.15"/>
                <inertia ixx="0.001859" ixy="-0.000376" ixz="0.000028" iyy="0.012756" iyz="0" izz="0.0024" />
            </inertial>
		</link>
		<link name="${prefix}link_3">
			<visual>
				<origin xyz="0 0 0" rpy="0 0 0.52"/>
				<geometry>
					<mesh filename="package://robotiq_hand_description/meshes/s-model_articulated/visual/link_3.STL" />
				</geometry>
				<material name="black"/>
			</visual>
			<collision>
	            <origin xyz="0.02 0.015 0" rpy="0 0 .52"/>
	            <geometry>
	                <box size=".025 .01 .03"/>
	            </geometry>
	            <material name="yellow"/>
	        </collision>
            <inertial>
                <origin xyz="0 0 0" rpy="0 0 0"/>
                <mass value="0.05"/>
                <inertia ixx="0.001239" ixy="-0.000251" ixz="0.000019" iyy="0.00085" iyz="0" izz="0.001632" />
            </inertial>
		</link>
		
		<link name="${prefix}fourbar_0_link_1">
            <inertial>
                <mass value="0.00001"/>
                <inertia ixx="0.00001"  ixy="0"  ixz="0" iyy="0.00001" iyz="0" izz="0.00001" />
            </inertial>
            <visual>
                <origin xyz=".01429 0 0" rpy="0 1.57 0"/>
                <geometry>
                    <cylinder length=".02858" radius="0.001"/>
                </geometry>
                <material>
