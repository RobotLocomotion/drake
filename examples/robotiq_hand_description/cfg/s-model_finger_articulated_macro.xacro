<!--
s-model_finder_articulated_macro(prefix)

Creates an articulated s-model finder with the passed in prefix.  The
prefix typcially includes the hand prefix and an identifier for the
finger(i.e. finger_1, etcpackage://robotiq_hand_description.).

-->
<robot xmlns:xacro="http://ros.org/wiki/xacro">
    <xacro:include filename="$(find robotiq_hand_description)/cfg/s-model_finger_fourbar_macro.xacro" />
	<xacro:macro name="s-model_finger_articulated" params="prefix">
		<!-- link list -->
		<link name="${prefix}link_0">
			<visual>
				<origin xyz="0.020 0 0" rpy="0 0 0"/>
				<geometry>
					<mesh filename="package://robotiq_hand_description/meshes/s-model_articulated/visual/link_0.STL" />
				</geometry>
				<material name="black">
					<color rgba=".2 .2 .2 1"/>
				</material>
			</visual>
			<!--<collision>	
				<origin xyz="-0.0455 0.0414 0.036" rpy="0 3.1416 -1.57"/>
				<geometry>
					<mesh filename="package://robotiq_hand_description/meshes/s-model_articulated/collision/link_0.STL" />
				</geometry>
				<material name="yellow">
					<color rgba="0 1 1 1"/>
				</material>
			</collision>-->
            <inertial>
                <origin xyz="0 0 0" rpy="0 0 0"/>
                <mass value="0.05"/>
                <inertia ixx="0.01" ixy="-0.00002" ixz="0.00001" iyy="0.0008" iyz="0" izz="0.001" />
            </inertial>         
		</link>
		<link name="${prefix}link_1">
			<visual>
				<origin xyz="0.050 -.028 0" rpy="0 0 -0.52"/>
				<geometry>
					<mesh filename="package://robotiq_hand_description/meshes/s-model_articulated/visual/link_1.STL" />
				</geometry>
				<material name="black"/>
			</visual>
			<!--<collision>
				<origin xyz="0.050 -.028 0" rpy="0 0 -0.52"/>
				<geometry>
					<mesh filename="package://robotiq_hand_description/meshes/s-model_articulated/collision/link_1.STL" />
				</geometry>
				<material name="yellow"/>
			</collision>-->
            <inertial>
                <origin xyz="0 0 0" rpy="0 0 0"/>
                <mass value="0.15"/>
                <inertia ixx="0.001859" ixy="-0.000376" ixz="0.000028" iyy="0.012756" iyz="0" izz="0.0024" />
            </inertial>
		</link>
		<link name="${prefix}link_2">
			<!--
			NOTE: This link has an offset in the z-direction based on the models that were
			used to generate grap-it poses.  With the offset below the model appears visually
			correct, but it is suspect.
			-->
			<visual>
				<origin xyz="0.039 0 0.0075" rpy="0 0 0"/>
				<geometry>
					<mesh filename="package://robotiq_hand_description/meshes/s-model_articulated/visual/link_2.STL" />
				</geometry>
				<material name="black"/>
			</visual>
			<!--<collision>
				<origin xyz="0.039 0 0.0075" rpy="0 0 0"/>
				<geometry>
					<mesh filename="package://robotiq_hand_description/meshes/s-model_articulated/collision/link_2.STL" />
				</geometry>
				<material name="yellow"/>
			</collision>-->
            <inertial>
                <origin xyz="0 0 0" rpy="0 0 0"/>
                <mass value="0.15"/>
                <inertia ixx="0.001859" ixy="-0.000376" ixz="0.000028" iyy="0.012756" iyz="0" izz="0.0024" />
            </inertial>
		</link>
		<link name="${prefix}link_3">
			<visual>
				<origin xyz="0 0 0" rpy="0 0 0.52"/>
				<geometry>
					<mesh filename="package://robotiq_hand_description/meshes/s-model_articulated/visual/link_3.STL" />
				</geometry>
				<material name="black"/>
			</visual>
			<!--<collision>
				<origin xyz="0 0 0" rpy="0 0 0.52"/>
				<geometry>
					<mesh filename="package://robotiq_hand_description/meshes/s-model_articulated/collision/link_3.STL" />
				</geometry>
				<material name="yellow"/>
			</collision>-->
            <inertial>
                <origin xyz="0 0 0" rpy="0 0 0"/>
                <mass value="0.05"/>
                <inertia ixx="0.001239" ixy="-0.000251" ixz="0.000019" iyy="0.00085" iyz="0" izz="0.001632" />
            </inertial>
		</link>

		<!-- end of link list -->
        <xacro:s-model_finger_fourbar prefix="${prefix}fourbar_0_" parent0="${prefix}link_1" parent1="${prefix}link_2" link_0_len="0.02858" link_1_len="0.05398" link_2_len="0.01473"/>
        <!--<xacro:s-model_finger_fourbar prefix="${prefix}fourbar_1_" parent0="${prefix}link_0" x0="0.2286" y0="0" parent1="${prefix}link_1" x1="0.2286" y1="0" theta="-1.57"/>-->
		<!-- joint list -->
		
		<!-- WARNING: LIMITS NOT CORRECT -->
		<joint name="${prefix}joint_1" type="revolute">
			<parent link="${prefix}link_0"/>
			<child link="${prefix}link_1"/>
			<axis xyz="0 0 1"/>
			<origin xyz="0.020 0 0" rpy="0 0 0"/>
			<limit lower="0" upper="3.1416" effort="100" velocity="100"/>
		</joint>
		<!-- WARNING: LIMITS NOT CORRECT -->
		<joint name="${prefix}joint_2" type="revolute">
			<parent link="${prefix}link_1"/>
			<child link="${prefix}link_2"/>
			<axis xyz="0 0 1"/>
			<origin xyz="0.050 -.028 0" rpy="0 0 -0.52"/>
			<limit lower="0" upper="3.1416" effort="100" velocity="100"/>
		</joint>
		<!-- WARNING: LIMITS NOT CORRECT -->
		<joint name="${prefix}joint_3" type="revolute">
			<parent link="${prefix}link_2"/>
			<child link="${prefix}link_3"/>
			<axis xyz="0 0 1"/>
			<origin xyz="0.039 0 0" rpy="0 0 0"/>
			<limit lower="0" upper="3.1416" effort="100" velocity="100"/>
		</joint>
		<!-- end of joint list -->
                <!-- gazebo property list -->
                <gazebo reference="${prefix}link_0">
                        <material>Gazebo/FlatBlack</material>
                        <turnGravityOff>true</turnGravityOff>
                </gazebo>
                <gazebo reference="${prefix}link_1">
                        <material>Gazebo/FlatBlack</material>
                        <turnGravityOff>true</turnGravityOff>
                </gazebo>
                <gazebo reference="${prefix}link_2">
                        <material>Gazebo/FlatBlack</material>
                        <turnGravityOff>true</turnGravityOff>
                </gazebo>
                <gazebo reference="${prefix}link_3">
                        <material>Gazebo/FlatBlack</material>
                        <turnGravityOff>true</turnGravityOff>
                </gazebo>
                <!-- end of gazebo property list -->
	</xacro:macro>
</robot>
