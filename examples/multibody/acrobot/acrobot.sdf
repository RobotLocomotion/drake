<sdf version='1.6'>
  <model name='Acrobot'>
    <link name='upper_link'>
      <pose>0 0.15 0 0 0 0</pose>
      <inertial>
        <pose>0 0 -0.5 0 0 0</pose>
        <mass>1</mass>
        <inertia>
          <ixx>1</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.083</iyy>
          <iyz>0</iyz>
          <izz>1</izz>
        </inertia>
      </inertial>
      <visual name='upper_link_visual'>
        <pose>0 0 -0.5 0 0 0</pose>
        <geometry>
          <cylinder>
            <length>1.1</length>
            <radius>0.05</radius>
          </cylinder>
        </geometry>
      </visual>
    </link>
    <joint name='shoulder' type='revolute'>
      <child>upper_link</child>
      <parent>world</parent>
      <axis>
        <xyz>0 1 0</xyz>
        <limit>
          <!-- Drake parses a zero effort joint as an un-actuated joint. -->
          <effort>0</effort>
        </limit>
        <dynamics>
          <damping>0.1</damping>
        </dynamics>
        <use_parent_model_frame>1</use_parent_model_frame>
      </axis>
    </joint>
    <link name='lower_link'>
      <pose>0 0.25 -1 0 0 0</pose>
      <inertial>
        <pose>0 0 -1 0 0 0</pose>
        <mass>1</mass>
        <inertia>
          <ixx>1</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.33</iyy>
          <iyz>0</iyz>
          <izz>1</izz>
        </inertia>
      </inertial>
      <visual name='lower_link_visual'>
        <pose>0 0 -1 0 0 0</pose>
        <geometry>
          <cylinder>
            <length>2.1</length>
            <radius>0.05</radius>
          </cylinder>
        </geometry>
      </visual>
    </link>
    <joint name='elbow' type='revolute'>
      <parent>upper_link</parent>
      <child>lower_link</child>
      <pose>0 0 -1 0 0 0</pose>
      <axis>
        <xyz>0 1 0</xyz>
        <limit>
          <!-- No limit provided. sdformat defaults to -1, which we parse as
               an actuated joint. -->
        </limit>
        <dynamics>
          <damping>0.1</damping>
        </dynamics>
        <use_parent_model_frame>1</use_parent_model_frame>
      </axis>
    </joint>
  </model>
</sdf>
