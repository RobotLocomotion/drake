<?xml version='1.0'?>
<sdf version='1.6'>
  <model name='Schunk_Gripper'>
    <!-- From contact_model/rigid_mug_gripper: Eigen::Vector3d(0.055 + 5e-4, 0, kMugHeight / 2 + 5e-4) -->
    <pose>0.0555 0 0.0505 0 0 1.57</pose>

    <joint name='weld_base' type='fixed'>
      <parent>world</parent>
      <child>y_translate_link</child>
      <pose>0 0 0 0 0 0</pose>
    </joint>

    <link name='y_translate_link'>
      <pose frame=''>0 0 0 0 -0 0</pose>
      <inertial>
        <mass>0.0001</mass>
        <inertia>
          <ixx>0.0001</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.0001</iyy>
          <iyz>0</iyz>
          <izz>0.0001</izz>
        </inertia>
        <pose frame=''>0 0 0 0 -0 0</pose>
      </inertial>
      <self_collide>0</self_collide>
      <kinematic>0</kinematic>
      <gravity>1</gravity>
    </link>

    <joint name='y_translate_joint' type='prismatic'>
      <parent>y_translate_link</parent>
      <child>body</child>
      <pose frame=''>0 0 0 0 -0 0</pose>
      <axis>
        <xyz>0 0 1</xyz>
        <use_parent_model_frame>1</use_parent_model_frame>
        <!-- Drake attaches an actuator to all and only joints with a nonzero
             effort limit. -->
        <limit>
          <lower>-1</lower>
          <upper>1</upper>
          <effort>80</effort>
          <stiffness>1500</stiffness>
          <dissipation>50</dissipation>
        </limit>
        <dynamics>
          <spring_reference>0</spring_reference>
          <spring_stiffness>0</spring_stiffness>
          <damping>0</damping>
          <friction>0</friction>
        </dynamics>
      </axis>
    </joint>

    <link name='body'>
      <pose frame=''>0 -0.049133 0 0 -0 0</pose>
      <inertial>
        <mass>0.988882</mass>
        <inertia>
          <ixx>0.162992</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.162992</iyy>
          <iyz>0</iyz>
          <izz>0.164814</izz>
        </inertia>
        <pose frame=''>0 0 0 0 -0 0</pose>
      </inertial>
      <visual name='visual'>
        <pose frame=''>0 0 0 0 -0 0</pose>
        <geometry>
          <box>
            <size>0.146 0.0725 0.049521</size>
          </box>
        </geometry>
      </visual>
    </link>

    <link name='left_finger'>
      <pose frame=''>0.040 0.029 0 0 -0 0</pose>
      <inertial>
        <mass>0.05</mass>
        <inertia>
          <ixx>0.16</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.16</iyy>
          <iyz>0</iyz>
          <izz>0.16</izz>
        </inertia>
        <pose frame=''>0 0 0 0 -0 0</pose>
      </inertial>
      <visual name='visual'>
        <pose frame=''>0 0 0 0 -0 0</pose>
        <geometry>
          <box>
            <size>0.007 0.081 0.028</size>
          </box>
        </geometry>
      </visual>
    </link>
    <link name='right_finger'> <!-- When viewing from the top and +x-axis points back -->
      <pose frame=''>0.047 0.029 0 0 0 0</pose>
      <inertial>
        <mass>0.05</mass>
        <inertia>
          <ixx>0.16</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.16</iyy>
          <iyz>0</iyz>
          <izz>0.16</izz>
        </inertia>
        <pose frame=''>0 0 0 0 -0 0</pose>
      </inertial>
      <visual name='visual'>
        <pose frame=''>0 0 0 0 -0 0</pose>
        <geometry>
          <box>
            <size>0.007 0.081 0.028</size>
          </box>
        </geometry>
      </visual>
    </link>

    <joint name='weld_right_finger' type='fixed'>
      <parent>body</parent>
      <child>right_finger</child>
      <pose>0 0 0 0 0 0</pose>
    </joint>

    <!-- Collision filter groups-->
    <collision_filter_group name="finger_filter">
      <member link="left_finger"/>
      <member link="right_finger"/>
      <ignored_collision_filter_group collision_filter_group="finger_filter"/>
    </collision_filter_group>
    <joint name='finger_sliding_joint' type='prismatic'>
      <parent>right_finger</parent>
      <child>left_finger</child>
      <pose frame=''>0 0 0 0 -0 0</pose>
      <axis>
        <xyz>1 0 0</xyz>
        <use_parent_model_frame>0</use_parent_model_frame>
        <!-- Drake attaches an actuator to all and only joints with a nonzero
             effort limit. -->
        <limit>
          <lower>-0.1</lower> <!-- offset w.r.t. absolute zero -->
          <upper>0</upper>
          <effort>90</effort>
        </limit>
      </axis>
    </joint>
  </model>
</sdf>
