<robot
  name="Glider">
  <link
    name="fuselage">
    <inertial>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <mass
        value="0.082" />
      <inertia
        ixx="0.1"
        ixy="0"
        ixz="0"
        iyy="0.003"
        iyz="0"
        izz="0.1" />
    </inertial>
    <visual>
      <origin
        xyz="-.135 0 0"
        rpy="0 0 0" />
      <geometry>
        <!--<mesh filename="meshes/GliderFuselage.obj" />-->
        <box size =".3 .1 .03"/>
      </geometry>
      <material
        name="">
        <color
          rgba="1 0 0 1" />
      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="meshes/GliderFuselage.STL" />
      </geometry>
    </collision>
  </link>
  <link
    name="Elevator">
    <inertial>
      <origin
        xyz="-0.022 0 0"
        rpy="0 0 0" />
<!--elevator needs to be (almost) massless and inertia-less in order
 for the dynamics to work-Rick's model assumes this. Ignore condition
 number warnings generated because of this.-->
      <mass
        value=".00001" />
      <inertia
        ixx="7.8055E-06"
        ixy="-3.0658E-08"
        ixz="1.5816E-08"
        iyy="8.2729E-07"
        iyz="-9.091E-09"
        izz="8.6162E-06" />
    </inertial>
    <visual>
      <origin
        xyz="-.02 0 0"
        rpy="0 0 0" />
      <geometry>
        <!--<mesh filename="meshes/GliderElevator.obj" />-->
        <box size=".08 .1 .01"/>
      </geometry>
      <material
        name="">
        <color
          rgba="0 0 1 1" />
      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="meshes/GliderElevator.STL" />
      </geometry>
    </collision>
  </link>
  <joint
    name="ElevatorHinge"
    type="revolute">
    <origin xyz="-0.270 0 0" rpy="0 0 0" />    
    <parent
      link="fuselage" />
    <child
      link="Elevator" />
    <axis
      xyz="0 1 0" />
    <limit
      effort="100"
      velocity="10" />
  </joint>
  <force_element name="Wing_force" type="wing"> 
  	<parent link="fuselage"/>
  	<profile value="flat plate"/> 
  	<origin xyz="0 0 0" rpy="0 0 0"/> 
  	<chord value=".08"/> 
  	<span value=".48125"/> 
  	<nominal_speed value="7"/> 
    <stall_angle value ="181"/>
  </force_element>
  <force_element name="Elevator_force" type="wing"> 
  	<parent link="Elevator"/>
  	<profile value="flat plate"/> 
  	<origin xyz="-.022 0 0" rpy="0 0 0"/> 
  	<chord value=".07"/> 
  	<span value=".21"/> 
  	<nominal_speed value="7"/> 
    <stall_angle value ="181"/>
  </force_element>
  <transmission name="ElevatorTorque" type="SimpleTransmission" >
    <joint name="ElevatorHinge" />
    <actuator name="ElevatorServo"/>
    <mechanicalReduction>1</mechanicalReduction>
  </transmission>
</robot>
