<?xml version="1.0" ?>

<robot name="box_hand" xmlns:xacro="http://www.ros.org/wiki/xacro">
  <link name="box_hand_1">
    <inertial>
      <!-- Total mass of robotiq hand from the associated urdf -->
      <mass value="2.3"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="0.001" ixy="0.0" ixz="0.0" iyy="0.001" iyz="0.0" izz="0.001"/>
    </inertial>
    <visual>
      <geometry>
        <box size="0.1 0.1 0.1"/>
      </geometry>
      <material name="gray">
        <color rgba="0.5 0.5 .5 1"/>
      </material>
      <origin rpy="0 0 0" xyz="0 0 0"/>
    </visual>
  </link>
</robot>
