<?xml version="1.0"?>
<sdf version="1.7">
  <model name="robot1">
    <link name="base_link">
      <visual name="Box">
        <geometry>
          <box>
            <size>0.1 0.1 0.1</size>
          </box>
        </geometry>
      </visual>
      <collision name="Sphere">
        <geometry>
          <sphere>
            <radius>0.2</radius>
          </sphere>
        </geometry>
      </collision>
    </link>
    <link name="moving_link"/>
    <joint name="slider" type="prismatic">
      <parent>base_link</parent>
      <child>moving_link</child>
      <axis>
        <xyz>0 0 1</xyz>
      </axis>
    </joint>
  </model>
</sdf>
