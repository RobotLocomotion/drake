      </inertial>
    </link>
    <joint name="universal_joint" type="universal">
      <pose>0 0 0 0 0 0</pose>
      <child>link6</child>
      <parent>link5</parent>
      <axis>
        <dynamics>
          <damping>0.1</damping>
        </dynamics>
        <use_parent_model_frame>1</use_parent_model_frame>
      </axis>
    </joint>
  </model>
</sdf>
