<?xml version="1.0" ?>
<!--
Loads five Prius models onto the ROS parameter server.
-->
<launch>
  <param name="car_description_1"
         command="$(find xacro)/xacro.py '$(find drake_cars_examples)/models/drake_prius/xacro/drake_prius_rviz.xacro'" />

  <param name="car_description_2"
         command="$(find xacro)/xacro.py '$(find drake_cars_examples)/models/drake_prius/xacro/drake_prius_rviz.xacro'" />

  <param name="car_description_3"
         command="$(find xacro)/xacro.py '$(find drake_cars_examples)/models/drake_prius/xacro/drake_prius_rviz.xacro'" />

  <param name="car_description_4"
         command="$(find xacro)/xacro.py '$(find drake_cars_examples)/models/drake_prius/xacro/drake_prius_rviz.xacro'" />

  <param name="car_description_5"
         command="$(find xacro)/xacro.py '$(find drake_cars_examples)/models/drake_prius/xacro/drake_prius_rviz.xacro'" />

  <param name="world_description"
         command="$(find xacro)/xacro.py '$(find drake_cars_examples)/models/drake_prius/xacro/planar_world.xacro'" />
</launch>