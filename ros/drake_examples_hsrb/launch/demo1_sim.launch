<?xml version="1.0"?>
<launch>
  <include file="$(find drake_examples_hsrb)/launch/load_model.launch" />

  <!--
    Sets the parameter that controls whether Drake publishes tf messages.
    Starts the simulation.
  -->
  <group ns="drake">
    <param name="enable_tf_publisher" type="bool" value="true" />
    <node pkg="drake_examples_hsrb" type="demo1" name="demo1" output="screen" />
  </group>
</launch>
