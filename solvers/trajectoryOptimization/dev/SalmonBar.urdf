<?xml version="1.0" ?>

<robot name="SalmonBar">
  <material name="Blue">
    <color rgba="0.2 0.2 0.9 1.0"/>
  </material>
  <material name="Green">
    <color rgba="0.2 0.9 0.2 1.0"/>
  </material>
  <material name="Red">
    <color rgba="0.9 0.2 0.2 1.0"/>
  </material>

  <link name="salmon_bar">
    <inertial>
      <mass value="0"/>
      <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0"/>
      <origin xyz="0 0 0"/>
    </inertial>

    <visual>
      <origin xyz = "0 -1 2" rpy="0 0 0"/>
      <geometry>
        <box size = "0.05 0.05 4"/>
	<!--<cylinder length="2" radius="0.05"/>-->
      </geometry>
      <material name="Blue"/>
    </visual>

    <collision>
      <origin xyz = "0 -1 2" rpy="0 0 0"/>
      <geometry>
        <box size = "0.05 0.05 4"/>
      </geometry>
    </collision>


    <visual>
      <origin xyz = "0 1 2" rpy="0 0 0"/>
      <geometry>
        <box size = "0.05 0.05 4"/>
	<!--<cylinder length="2" radius="0.05"/>-->
      </geometry>
      <material name="Blue"/>
    </visual>

    <collision>
      <origin xyz = "0 1 2" rpy="0 0 0"/>
      <geometry>
        <box size = "0.05 0.05 4"/>
      </geometry>
    </collision>
  </link>
</robot>
