      <mass value="14.28"/>
      <inertia ixx=".49991220" iyy=".77843904" izz=".49991220"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 2.0"/>
      <geometry>
        <box size="5.0 2.5 4.0"/>
      </geometry>
    </visual>
  </link>
</robot>
