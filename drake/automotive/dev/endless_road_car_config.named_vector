element {
    name: "wheelbase"
    doc: "distance between left/right wheels [positive value, meters]"
}
element {
    name: "max_abs_steering_angle"
    doc: "maximum absolute front wheel angle (from forward) [positive value, radians]"
}
element {
    name: "max_velocity"
    doc: "maximum allowed forward velocity [positive value, m/s]"
}
element {
    name: "max_acceleration"
    doc: "maximum forward acceleration [positive value, m/s^2]"
}
element {
    name: "max_deceleration"
    doc: "maximum forward deceleration [positive value, m/s^2]"
}
