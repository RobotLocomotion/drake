<?xml version="1.0"?>
<robot xmlns="http://drake.mit.edu"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://drake.mit.edu ../../../../doc/drakeURDF.xsd" name="PointMass">
  <link name="point">
    <inertial>
      <mass value="1"/>
      <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
    </inertial>
    <visual>
      <geometry>
        <sphere radius="0.1" />
      </geometry>
    </visual>
    <collision>
      <geometry>
        <sphere radius="0.0" />
      </geometry>
    </collision>
  </link>
</robot>

