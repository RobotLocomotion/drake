<?xml version='1.0'?>
<!-- 
This model describes a mechanism to be attached to the "world" (at the base),
with six degrees of freedom: three translational and three rotational. Each
degree of freedom is realized through a single degree of freedom joint- revolute
or prismatic- connecting two low-mass (1e-4 kg) links. 
-->

<sdf version='1.6'>
  <model name='Test lifter_and_shaker'>
    <link name='base'/>
    <link name='lifted_link'>
      <pose frame=''>0 0 0 0 -0 0</pose>
      <inertial>
        <mass>0.0001</mass>
        <inertia>
          <ixx>0.0001</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.0001</iyy>
          <iyz>0</iyz>
          <izz>0.0001</izz>
        </inertia>
        <pose frame=''>0 0 0 0 -0 0</pose>
      </inertial>
      <self_collide>0</self_collide>
      <kinematic>0</kinematic>
      <gravity>1</gravity>
    </link>

    <link name='x_translate_link'>
      <pose frame=''>0 0 0 0 -0 0</pose>
      <inertial>
        <mass>0.0001</mass>
        <inertia>
          <ixx>0.0001</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.0001</iyy>
          <iyz>0</iyz>
          <izz>0.0001</izz>
        </inertia>
        <pose frame=''>0 0 0 0 -0 0</pose>
      </inertial>
      <self_collide>0</self_collide>
      <kinematic>0</kinematic>
      <gravity>1</gravity>
    </link>

    <joint name='lift_joint' type='prismatic'>
      <parent>base</parent>
      <child>lifted_link</child>
      <pose frame=''>0 0 0 0 -0 0</pose>
      <axis>
        <xyz>0 0 1</xyz>
        <use_parent_model_frame>0</use_parent_model_frame>
        <!-- Drake attaches an actuator to all and only joints with a nonzero
             effort limit. -->
        <limit>
          <lower>0</lower>
          <upper>1</upper>
          <effort>80</effort>
          <stiffness>1500</stiffness>
          <dissipation>50</dissipation>
        </limit>
        <dynamics>
          <spring_reference>0</spring_reference>
          <spring_stiffness>0</spring_stiffness>
          <damping>0</damping>
          <friction>0</friction>
        </dynamics>
      </axis>
    </joint>

    <joint name='x_translate_joint' type='prismatic'>
      <parent>lifted_link</parent>
      <child>x_translate_link</child>
      <pose frame=''>0 0 0 0 -0 0</pose>
      <axis>
        <xyz>1 0 0</xyz>
        <use_parent_model_frame>0</use_parent_model_frame>
        <!-- Drake attaches an actuator to all and only joints with a nonzero
             effort limit. -->
        <limit>
          <lower>-1</lower>
          <upper>1</upper>
          <effort>80</effort>
          <stiffness>1500</stiffness>
          <dissipation>50</dissipation>
        </limit>
        <dynamics>
          <spring_reference>0</spring_reference>
          <spring_stiffness>0</spring_stiffness>
          <damping>0</damping>
          <friction>0</friction>
        </dynamics>
      </axis>
    </joint>

  </model>
</sdf>
